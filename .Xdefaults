! Appearance
urxvt*font:              xft:Consolas:pixelsize=16:antialias=true:hinting=false
urxvt*boldfont:          xft:Consolas:pixelsize=16:antialias=true:hinting=false
urxvt.perl-ext-common:   default,tabbedex

urxvt.depth:             32

URxvt.transparent:       false

URxvt.scrollBar:         false
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true
urxvt*secondaryScroll:   true
urxvt*saveLines:         65535

URxvt*keysym.Home:       \033[1~
URxvt*keysym.End:        \033[4~

*background: #00161c
*foreground: #a5bbc3

! black dark/light
*color0: #073642
*color8: #002b36
 
!! red dark/light
*color1: #dc322f
*color9: #cb4b16
 
!! green dark/light
*color2: #859900
*color10: #586e75
 
!! yellow dark/light
*color3: #b58900
*color11: #657b83
 
!! blue dark/light
*color4: #245695
*color12: #839496
 
!! magenta dark/light
*color5: #d33682
*color13: #6c71c4
 
!! cyan dark/light
*color6: #2aa198
*color14: #93a1a1
 
!! white dark/light
